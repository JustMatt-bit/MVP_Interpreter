print("Fibonacci series made of idk how many numbers using recursive fuctions");
fun int testas <int n> {
    if(n <= 1){
        return n;
    }
    else{
        int recOne(n - 1);
        int recTwo(n - 2);
        int fib(testas<recOne> + testas<recTwo>);
    return fib;
    }
}
int b(12);
int a(testas<b>);
print(a);

int count(2);
int numOne(0);
int numTwo(1);
print("Fibonacci series made of 20 numbers using for loop");

for (int i(1); i(i + 1); i <= count) {
    print(numOne);
    int sumOfPrevTwo(numOne + numTwo);
    numOne(numTwo);
    numTwo(sumOfPrevTwo);
}

